build-type:          Simple
extra-source-files:  ChangeLog.md, README.md
cabal-version:       >=1.10

executable ukt
  main-is:             ../test/Main.hs
  -- other-modules:
  other-extensions:    TupleSections
  					 , FlexibleInstances
  					 , UndecidableInstances
  					 , TypeSynonymInstances
  					 , MultiParamTypeClasses
  					 , FunctionalDependencies
  					 , FlexibleContexts
  					 , TypeOperators
  build-depends:       base >=4.11 && <4.12
  					 , containers >=0.5 && <0.6
  					 , process >=1.6 && <1.7
  					 , temporary >=1.3 && <1.4
  					 , text >=1.2 && <1.3
  					 , fgl >=5.6 && <5.7
  					 , graphviz >=2999.20 && <2999.21
					 , mtl >= 2.2.2
					 , directory >= 1.3.3.2
					 , random >= 1.1
  hs-source-dirs:      src, test, src/programs, utils
  default-language:    Haskell2010
